{
  "name": "shinkansen-signals",
  "version": "1.2.47",
  "description": "Shinkansen Signals",
  "keywords": [
    "Shinkansen",
    "Signals",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "main": "./src/index.mjs",
  "type": "module",
  "types": "./index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/modernpoacher/shinkansen-signals.git"
  },
  "scripts": {
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "nvm": "bash nvm.sh",
    "prelint": "npm run nvm",
    "prepare": "husky",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test mocha test --recursive"
  },
  "dependencies": {
    "debug": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/preset-env": "^7.27.2",
    "@babel/register": "^7.27.1",
    "@sequencemedia/eslint-config-standard": "^0.2.64",
    "@sequencemedia/eslint-config-typescript": "^0.1.101",
    "@types/babel__register": "^7.17.3",
    "@types/chai": "^5.2.2",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "chai": "^5.2.0",
    "core-js": "^3.43.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "mocha": "^11.6.0"
  },
  "imports": {
    "#signals": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "#signals/signals": "./src/signals/index.mjs",
    "#signals/signals/*": "./src/signals/*.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./signals": "./src/signals/index.mjs",
    "./signals/signals/*": "./src/signals/*.mjs"
  }
}
